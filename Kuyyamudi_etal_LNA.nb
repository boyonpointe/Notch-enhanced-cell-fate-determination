Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"SSuncoupled", "[", 
    RowBox[{"Mss_", ",", "tmax_"}], "]"}], ":=", "\[IndentingNewLine]", 
   RowBox[{"(", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"nvars", "=", "3"}], ";", " ", 
     RowBox[{"(*", " ", 
      RowBox[{
      "each", " ", "cell", " ", "is", " ", "described", " ", "by", " ", "3", 
       " ", "variables"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"n", "=", 
      RowBox[{
       RowBox[{"Dimensions", "[", 
        RowBox[{"Mss", ",", "1"}], "]"}], "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "for", " ", "storing", " ", "the", " ", "steady", " ", "states", " ", 
       "at", " ", "different", " ", "locations", " ", "along", " ", "the", 
       " ", "gradient"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"Xss", "=", 
      RowBox[{"Array", "[", 
       RowBox[{
        RowBox[{"0", "&"}], ",", 
        RowBox[{"{", 
         RowBox[{"nvars", ",", "n"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"the", " ", "Hill", " ", "function", " ", 
       FractionBox[
        SuperscriptBox["Q", "g"], 
        RowBox[{
         SuperscriptBox["Q", "g"], "+", 
         SuperscriptBox["S", "g"]}]], " ", "is", " ", "not", " ", "present", 
       " ", "in", " ", "the", " ", "uncoupled", " ", "system"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"Sval", "=", "0"}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"cell", "=", "1"}], ",", " ", 
       RowBox[{"cell", "<", 
        RowBox[{"n", "+", "1"}]}], ",", " ", 
       RowBox[{"cell", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"\[Alpha]M", "=", 
         RowBox[{"Mss", "[", 
          RowBox[{"[", "cell", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"sol", "=", 
         RowBox[{"NDSolve", "[", 
          RowBox[{
           RowBox[{"{", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"A", "'"}], "[", "t", "]"}], "\[Equal]", 
              RowBox[{"E1", "[", 
               RowBox[{
                RowBox[{"A", "[", "t", "]"}], ",", 
                RowBox[{"B", "[", "t", "]"}], ",", 
                RowBox[{"M", "[", "t", "]"}]}], "]"}]}], ",", 
             "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{
               RowBox[{"B", "'"}], "[", "t", "]"}], "\[Equal]", 
              RowBox[{"E2", "[", 
               RowBox[{
                RowBox[{"A", "[", "t", "]"}], ",", 
                RowBox[{"B", "[", "t", "]"}], ",", "Sval", ",", 
                RowBox[{"M", "[", "t", "]"}]}], "]"}]}], ",", 
             "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{
               RowBox[{"M", "'"}], "[", "t", "]"}], "\[Equal]", 
              RowBox[{"E6", "[", 
               RowBox[{
                RowBox[{"M", "[", "t", "]"}], ",", "\[Alpha]M"}], "]"}]}], 
             ",", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"A", "[", "0", "]"}], "\[Equal]", "0"}], ",", 
             RowBox[{
              RowBox[{"B", "[", "0", "]"}], "\[Equal]", "0"}], ",", 
             RowBox[{
              RowBox[{"M", "[", "0", "]"}], "\[Equal]", "\[Alpha]M"}]}], 
            "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"A", ",", "B", ",", "M"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"t", ",", "0", ",", "tmax"}], "}"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "steady", " ", "state", " ", "values", " ", "at", " ", "each", " ", 
          "location", " ", "along", " ", "the", " ", "domain"}], " ", "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"Xss", "[", 
           RowBox[{"[", "1", "]"}], "]"}], "[", 
          RowBox[{"[", "cell", "]"}], "]"}], "=", 
         RowBox[{
          RowBox[{"Evaluate", "[", 
           RowBox[{
            RowBox[{"A", "[", "tmax", "]"}], "/.", "sol"}], "]"}], "[", 
          RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"Xss", "[", 
           RowBox[{"[", "2", "]"}], "]"}], "[", 
          RowBox[{"[", "cell", "]"}], "]"}], "=", 
         RowBox[{
          RowBox[{"Evaluate", "[", 
           RowBox[{
            RowBox[{"B", "[", "tmax", "]"}], "/.", "sol"}], "]"}], "[", 
          RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"Xss", "[", 
           RowBox[{"[", "3", "]"}], "]"}], "[", 
          RowBox[{"[", "cell", "]"}], "]"}], "=", 
         RowBox[{
          RowBox[{"Evaluate", "[", 
           RowBox[{
            RowBox[{"M", "[", "tmax", "]"}], "/.", "sol"}], "]"}], "[", 
          RowBox[{"[", "1", "]"}], "]"}]}], ";"}]}], "\[IndentingNewLine]", 
      "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"output", "=", "Xss"}]}], "\[IndentingNewLine]", ")"}]}], 
  ";"}]], "Input",
